# redirect subdomain to subdomain folder in *.domain.com document root
RewriteEngine On 
RewriteCond %{HTTP_HOST} ^(.+)\.boo\.loc$ [NC]
RewriteCond %{HTTP_HOST} !^www\.
RewriteRule ^([^/]*)$ /%1%{REQUEST_URI}?folder=%1 [L,QSA]

